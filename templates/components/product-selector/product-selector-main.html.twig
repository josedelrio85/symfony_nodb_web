<div class="product-selector">
  <div id="mainelements" class="row">
    {% for product in mainelements %}
      <div id="product-{{product}}" class="col-12 col-md-6 mb-3 product-btn">
        <div class="inner d-flex justify-content-between bg-primary align-items-center p-4">
          <h5 class="text-light m-0 name">{{product}}</h5>
          <span class="icon icon-heart text-light h5 mb-0"></span>
        </div>
      </div>
    {% endfor %}
  </div>
</div>