<div class="product-selector">
  {# <h1> product-selector.html.twig</h1> #}

  <div id="mainelements" class="row">
    {% for product in mainelements %}
      <div id="product-{{product}}" class="col-12 col-md-6 mb-3 product-button">
        <div class="inner d-flex justify-content-between bg-primary align-items-center p-4">
          <h5 class="text-light m-0 name">{{product}}</h5>
          <span class="icon icon-heart text-light h5 mb-0"></span>
        </div>
      </div>
    {% endfor %}
  </div>

  <div id="firstchildrens" class="d-none container">
    <div  class="row">
      <p id="back-0">atras</p>
      <div  class="container">


            {% for key, product in firstchildrens %}
              <div id="fc-{{key}}" class="d-none row">

                {# <h2>firstchildrens - {{key}}</h2> #}
                {% for child in product %}
                  <div id="{{child.name}}" class="inner col-12 col-md-6 product-button">
                    <div class="inner justify-content-between bg-primary align-items-center p-4">
                      {% set path = null %}
                      {% if child.path is defined %}
                        {% set path = child.path %}
                      {% endif %}
                      <h5 class="text-light m-0 name launch {{path}}">{{child.name}}</h5>
                    </div>
                  </div>
                {% endfor %}

              </div>
            {% endfor %}

      </div>
    </div>
  </div>

  <div id="secondchildrens" class="d-none row">
    <p id="back-1">atras</p>

    {% for key, product in secondChildrens %}

      <div id="sc-{{key}}" class="d-none">
        <h2>secondchildrens - {{key}}</h2>

        {% for child in product %}
          <div id="{{child.name}}" class="">
            <h5 class="text-light m-0 name launch {{child.path}}">{{child.name}}</h5>
          </div>
        {% endfor %}
      </div>
    {% endfor %}
  </div>
</div>